# Copy this file to .env and fill in values. All keys are optional.
# The app runs without them; these only enable the monitoring pipeline in alert.py.

# Sentinel Hub (for NDVI imagery)
SENTINEL_CLIENT_ID=
SENTINEL_CLIENT_SECRET=

# Pinata (for IPFS uploads)
PINATA_API_KEY=
PINATA_SECRET_API_KEY=

# Ethereum (Sepolia) for on-chain logging
# If INFURA_URL is set but PRIVATE_KEY is empty, on-chain logging is skipped safely.
INFURA_URL=
ACCOUNT_ADDRESS=
PRIVATE_KEY=
CONTRACT_ADDRESS=
